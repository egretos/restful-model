{
    "name": "egretos/restful-model",
    "description": "Restful API model for easy implementation of any RESTful API",
    "keywords": [
        "laravel",
        "api",
        "model",
        "http",
        "rest",
        "restful"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ihor Melnychuk",
            "email": "egretos@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Egretos\\RestModel\\": "src/Model/"
        }
    },
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "jenssegers/model": "^1.3",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "illuminate/contracts": "5.8.* || ^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "5.8.* || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "5.8.* || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.* || ^4.0 || ^5.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Egretos\\RestModel\\RestModelServiceProvider"
            ]
        }
    }
}
